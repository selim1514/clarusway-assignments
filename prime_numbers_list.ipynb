   "source": [
    "n = int(input(\"Enter a positive number: \")) \n",
    "prime_list = []\n",
    "for num in range(2,n+1):\n",
    "    prime = True\n",
    "    for i in range(2,num):\n",
    "        if (num%i==0):\n",
    "            prime = False\n",
    "    if prime:\n",
    "        prime_list.append(num)\n",
    "print(prime_list)\n",
    "       "
   ]
